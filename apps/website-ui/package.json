{
  "name": "@monorepo/website-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm vite --open",
    "dev-ui": "pnpm dev",
    "build": "pnpm vite build && pnpm exec node ./scripts/post-build.mjs ",
    "ios": "pnpm build && pnpm sync && cap open ios",
    "android": "pnpm build && pnpm sync && cap open android",
    "remove-source-map": "rimraf ./dist/client/assets/**/*.js.map",
    "sync": "cap sync",
    "cap-ios": "cap add ios",
    "cap-android": "cap add android",
    "env-set-mobile": "ENV_TYPE=mobile node ./scripts/mobile-env.mjs",
    "env-set-web": "ENV_TYPE=web node ./scripts/mobile-env.mjs",
    "analyze": "ANALYZE=true pnpm run build-prod",
    "preview-wesite": "pnpm dlx serve ./dist/client",
    "preview-mobile": "pnpm dlx serve ./dist",
    "capacitor-assets": "capacitor-assets generate --assetPath ./mobile-assets",
    "generate-svg-sprite": "pnpm exec node ./scripts/generate-svg-icons.js",
    "generate-sitemap": "pnpm exec node ./scripts/sitemaps.mjs",
    "generate-robots": "pnpm exec node ./scripts/generate-robots.mjs",
    "clean-cache": "rm -rf node_modules/.cache node_modules/.vite .eslintcache tsconfig.tsbuildinfo .turbo",
    "clean-artifacts": "rm -rf dist build",
    "storybook": "pnpm storybook dev -p 3002",
    "storybook-build": "pnpm storybook build",
    "prettier-fix": "pnpm prettier **/* --write --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore",
    "prettier-check": "pnpm prettier **/* --check --no-error-on-unmatched-pattern --ignore-path ../../.prettierignore",
    "eslint-check": "pnpm eslint --cache --report-unused-disable-directives ./src",
    "eslint-fix": "pnpm eslint --cache --report-unused-disable-directives ./src --fix",
    "ts-check": "pnpm tsc --noEmit",
    "ts-report": "rimraf .coverage/typescript && mkdir -p .coverage/typescript && typescript-coverage-report -s -t 95 -o .coverage/typescript",
    "test": "pnpm jest --no-cache",
    "test-silent": "pnpm jest --no-cache --silent",
    "test-report": "pnpm jest --no-cache --coverage",
    "qa": "pnpm run prettier-check && pnpm run eslint-check && pnpm run ts-check && pnpm run test && echo âœ… Success",
    "fix": "pnpm run prettier-fix && pnpm run eslint-fix && pnpm run qa"
  },
  "devDependencies": {
    "@sentry/react": "^8.4.0",
    "@capacitor-community/admob": "^6.0.0",
    "@capacitor-community/firebase-analytics": "^6.0.0",
    "@capacitor/android": "^6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/dialog": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capawesome/capacitor-app-update": "^6.0.0",
    "@capgo/inappbrowser": "^6.0.10",
    "@fullstory/browser": "^2.0.4",
    "@hookform/resolvers": "^3.4.2",
    "@monorepo/eslint-config": "workspace:*",
    "@monorepo/jest-config": "workspace:*",
    "@monorepo/storybook-config": "workspace:*",
    "@monorepo/web-toolkit": "workspace:*",
    "@sentry/fullstory": "^2.1.0",
    "@tanstack/react-router": "^1.33.8",
    "@tanstack/router-devtools": "^1.33.8",
    "@tanstack/router-vite-plugin": "1.33.12",
    "@types/google-publisher-tag": "^1.20240219.0",
    "@types/node": "^20.12.12",
    "@types/qs": "^6.9.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/scroll-lock": "^2.1.3",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "capacitor-plugin-safe-area": "^3.0.1",
    "clsx": "^2.1.1",
    "lite-youtube-embed": "^0.3.2",
    "cross-fetch": "^4.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "idb-keyval": "^6.2.1",
    "jotai": "2.8.1",
    "keen-slider": "^6.8.6",
    "postcss": "^8.4.38",
    "query-string": "^9.0.0",
    "rc-slider": "^10.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.51.5",
    "react-viewport-list": "^7.1.2",
    "rimraf": "5.0.7",
    "rmx-cli": "^0.4.16",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.2",
    "schema-dts": "^1.1.2",
    "semver": "^7.6.2",
    "sitemap": "^8.0.0",
    "vike": "^0.4.171",
    "vite-imagetools": "^7.0.2",
    "typescript": "^5.4.5",
    "vite": "5.2.11"
  },
  "engines": {
    "node": ">=18.x"
  }
}
